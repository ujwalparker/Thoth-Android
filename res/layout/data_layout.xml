<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:listSelector="@android:color/transparent"
	android:id="@+id/data_main">
	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="60dp"
		android:id="@+id/t_d_total_out_holder"
		android:padding="5dp">
		<TextView
			android:layout_width="wrap_content"
			android:minWidth="120dp"
			android:layout_height="fill_parent"
			android:id="@+id/t_d_total_out_label_id"
			android:text="@string/t_d_total_out_label"
			android:layout_alignParentLeft="true"
			android:textSize="18dp"
			android:layout_alignParentTop="true"
			android:textColor="#FFFFFF"></TextView>
		<ImageView
			android:layout_margin="4dp"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:id="@+id/t_d_total_out_id"
			android:layout_height="fill_parent"
			android:layout_alignParentTop="true"
			android:src="@drawable/t_d_total_out"></ImageView>
		<TextView
			android:layout_toLeftOf="@+id/t_d_total_out_id"
			android:layout_width="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_height="wrap_content"
			android:layout_marginRight="4dp"
			android:id="@+id/t_d_total_out_data_id"
			android:text="@string/t_d_total_out_data"
			
			android:layout_alignParentBottom="true"
			android:textColor="#FFFFFF"></TextView>
	</RelativeLayout>
	<include
		layout="@layout/gradient_view_one" />
	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="60dp"
		android:id="@+id/t_d_total_in_holder"
		android:padding="5dp">
		<TextView
			android:layout_width="wrap_content"
			android:minWidth="120dp"
			android:layout_height="fill_parent"
			android:id="@+id/t_d_total_in_label_id"
			android:text="@string/t_d_total_in_label"
			android:layout_alignParentLeft="true"
			android:textSize="18dp"
			android:layout_alignParentTop="true"
			android:textColor="#FFFFFF"></TextView>
		<ImageView
			android:layout_margin="4dp"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:id="@+id/t_d_total_in_id"
			android:layout_height="fill_parent"
			android:layout_alignParentTop="true"
			android:src="@drawable/t_d_total_in"></ImageView>
		<TextView
			android:layout_toLeftOf="@+id/t_d_total_in_id"
			android:layout_width="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_height="wrap_content"
			android:layout_marginRight="4dp"
			android:id="@+id/t_d_total_in_data_id"
			android:text="@string/t_d_total_in_data"
			
			android:layout_alignParentBottom="true"
			android:textColor="#FFFFFF"></TextView>
	</RelativeLayout>
	<include
		layout="@layout/gradient_view_one" />
	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="60dp"
		android:id="@+id/t_d_mobile_out_holder"
		android:padding="5dp">
		<TextView
			android:layout_width="wrap_content"
			android:minWidth="120dp"
			android:layout_height="fill_parent"
			android:id="@+id/t_d_mobile_out_label_id"
			android:text="@string/t_d_mobile_out_label"
			android:layout_alignParentLeft="true"
			android:textSize="18dp"
			android:layout_alignParentTop="true"
			android:textColor="#FFFFFF"></TextView>
		<ImageView
			android:layout_margin="4dp"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:id="@+id/t_d_mobile_out_id"
			android:layout_height="fill_parent"
			android:layout_alignParentTop="true"
			android:src="@drawable/t_d_mobile_out"></ImageView>
		<TextView
			android:layout_toLeftOf="@+id/t_d_mobile_out_id"
			android:layout_width="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_height="wrap_content"
			android:layout_marginRight="4dp"
			android:id="@+id/t_d_mobile_out_data_id"
			android:text="@string/t_d_mobile_out_data"
			
			android:layout_alignParentBottom="true"
			android:textColor="#FFFFFF"></TextView>
	</RelativeLayout>
	<include
		layout="@layout/gradient_view_one" />
	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="60dp"
		android:id="@+id/t_d_mobile_in_holder"
		android:padding="5dp">
		<TextView
			android:layout_width="wrap_content"
			android:minWidth="120dp"
			android:layout_height="fill_parent"
			android:id="@+id/t_d_mobile_in_label_id"
			android:text="@string/t_d_mobile_in_label"
			android:layout_alignParentLeft="true"
			android:textSize="18dp"
			android:layout_alignParentTop="true"
			android:textColor="#FFFFFF"></TextView>
		<ImageView
			android:layout_margin="4dp"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:id="@+id/t_d_mobile_in_id"
			android:layout_height="fill_parent"
			android:layout_alignParentTop="true"
			android:src="@drawable/t_d_mobile_in"></ImageView>
		<TextView
			android:layout_toLeftOf="@+id/t_d_mobile_in_id"
			android:layout_width="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_height="wrap_content"
			android:layout_marginRight="4dp"
			android:id="@+id/t_d_mobile_in_data_id"
			android:text="@string/t_d_mobile_in_data"
			
			android:layout_alignParentBottom="true"
			android:textColor="#FFFFFF"></TextView>
	</RelativeLayout>
	<include
		layout="@layout/gradient_view_one" />
</LinearLayout>